# Environment Variables for AI Book Generation Project
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key for GPT models
OPENAI_API_KEY=enter_your_openai_api_key_here

# Replicate API Token for image generation models
REPLICATE_API_TOKEN=enter_your_replicate_api_key_here

# PayPal Configuration
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=enter_your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=enter_your_paypal_client_secret_here
# Apple Pay Sandbox
APPLE_PAY_MERCHANT_ID=merchant.com.high5.genbook.sandbox
APPLE_PAY_MERCHANT_DOMAIN=localhost:3000
APPLE_PAY_SANDBOX=true

# Google Pay Sandbox
GOOGLE_PAY_MERCHANT_ID=test_merchant_id
GOOGLE_PAY_MERCHANT_NAME=High5 Gen Book
GOOGLE_PAY_GATEWAY=example
GOOGLE_PAY_ENVIRONMENT=TEST

# Amazon Pay Sandbox
AMAZON_PAY_MERCHANT_ID=test_merchant_id
AMAZON_PAY_PUBLIC_KEY_ID=test_public_key
AMAZON_PAY_STORE_ID=test_store_id
AMAZON_PAY_REGION=US
AMAZON_PAY_SANDBOX=true
